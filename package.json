{
  "name": "react-visual-health",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.51",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "autoprefixer": "7.1.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0-beta.4",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "^1.6.0",
    "css-loader": "^2.1.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^0.14.3",
    "jest": "^24.7.1",
    "koa-connect": "^2.0.1",
    "lint-staged": "^7.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.1",
    "moment-duration-format": "^2.2.2",
    "prettier": "^1.12.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.3.2",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.8.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "recharts": "^1.5.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "resolve": "1.6.0",
    "style-loader": "0.21.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^1.0.4",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "webpack-serve --config config/webpack.config.dev.js",
    "build": "rm -rf build; webpack-cli --config config/webpack.config.prod.js",
    "lint": "eslint src/**/*.js --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "serviceworker-webpack-plugin": "^1.0.0-alpha02"
  }
}
